<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Payment">
            <file name="org-cashforward-ui-action-EnterScheduledPaymentAction.instance"/>
            <file name="org-cashforward-ui-action-NewPaymentAction.instance"/>
            <file name="org-cashforward-ui-action-SkipScheduledPaymentAction.instance"/>
        </folder>
        <folder name="Window">
            <file name="org-cashforward-ui-payment-PaymentAction.instance"/>
            <file name="org-cashforward-ui-scenario-ScenarioAction.instance"/>
            <file name="org-cashforward-ui-task-TaskAction.instance"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="PaymentAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-cashforward-ui-payment-PaymentAction.instance"/>
            </file>
            <file name="ScenarioAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-cashforward-ui-scenario-ScenarioAction.instance"/>
            </file>
            <file name="TaskAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-cashforward-ui-task-TaskAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="File">
            <file name="org-cashforward-ui-action-EnterScheduledPaymentAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Payment/org-cashforward-ui-action-EnterScheduledPaymentAction.instance"/>
                <attr name="position" intvalue="200"/>
            </file>
            <file name="org-cashforward-ui-action-NewPaymentAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Payment/org-cashforward-ui-action-NewPaymentAction.instance"/>
                <attr name="position" intvalue="300"/>
            </file>
            <file name="org-cashforward-ui-action-SkipScheduledPaymentAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Payment/org-cashforward-ui-action-SkipScheduledPaymentAction.instance"/>
                <attr name="position" intvalue="100"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="PaymentTopComponent.settings" url="PaymentTopComponentSettings.xml"/>
            <file name="ScenarioTopComponent.settings" url="ScenarioTopComponentSettings.xml"/>
            <file name="TaskTopComponent.settings" url="TaskTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="ScenarioTopComponent.wstcref" url="ScenarioTopComponentWstcref.xml"/>
            </folder>
            <folder name="explorer">
                <file name="PaymentTopComponent.wstcref" url="PaymentTopComponentWstcref.xml"/>
            </folder>
            <folder name="leftSlidingSide">
                <file name="TaskTopComponent.wstcref" url="TaskTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
